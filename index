    const loginView = document.getElementById('login-view');
    const dashboardView = document.getElementById('dashboard-view');
    const loginForm = document.getElementById('login-form');
    const emailInput = document.getElementById('email-input');
    const passwordInput = document.getElementById('password-input');
    const loginError = document.getElementById('login-error');

    const userNameEl = document.getElementById('user-name');
    const userEmailEl = document.getElementById('user-email');
    const userIdEl = document.getElementById('user-id');
    const userAvatarEl = document.getElementById('user-avatar');

    const statRateEl = document.getElementById('stat-rate');
    const statCompletedEl = document.getElementById('stat-completed');
    const statTotalEl = document.getElementById('stat-total');
    const progressValueEl = document.getElementById('progress-value');
    const progressBarEl = document.getElementById('progress-bar');
    const progressLabelEl = document.getElementById('progress-label');
    const modulesSectionEl = document.getElementById('modules-section');

    const logoutBtn = document.getElementById('logout-btn');
    const printBtn = document.getElementById('print-btn');

    function normalizeEmail(email) {
      return (email || '').trim().toLowerCase();
    }

    function findStudent(email, password) {
      const normEmail = normalizeEmail(email);
      const pwd = String(password || '').trim();
      if (!normEmail || !pwd) return null;

      return students.find(s => {
        return normalizeEmail(s.email) === normEmail && 
               String(s.password || '').trim() === pwd;
      }) || null;
    }

    function getInitials(name) {
      if (!name) return '??';
      const parts = name.trim().split(/\s+/);
      if (parts.length === 1) return parts[0].slice(0, 2).toUpperCase();
      return (parts[0][0] + parts[parts.length - 1][0]).toUpperCase();
    }

    function getModuleIcon(moduleName) {
      if (moduleName.includes('1')) return '①';
      if (moduleName.includes('2')) return '②';
      if (moduleName.includes('3')) return '③';
      if (moduleName.includes('4')) return '④';
      if (moduleName.includes('5')) return '⑤';
      if (moduleName.includes('6')) return '⑥';
      if (moduleName.includes('Évaluation')) return '📋';
      if (moduleName.includes('Forum')) return '💬';
      return '📚';
    }

    function calculateModuleCompletion(activities) {
      const total = Object.keys(activities).length;
      if (total === 0) return 0;
      
      let completed = 0;
      Object.values(activities).forEach(status => {
        if (status.toLowerCase().includes('terminé') && status !== 'Pas terminé') {
          completed++;
        }
      });
      
      return Math.round((completed / total) * 100);
    }

    function getModuleBadge(completion) {
      if (completion === 100) return '<span class="module-badge complete">✓ Terminé</span>';
      if (completion > 0) return '<span class="module-badge progress">En cours</span>';
      return '<span class="module-badge incomplete">Non commencé</span>';
    }

    function updateDashboard(student) {
      currentStudent = student;

      userNameEl.textContent = student.name || '';
      userEmailEl.textContent = student.email || '';
      userIdEl.textContent = student.id ? `ID : ${student.id}` : '';
      userAvatarEl.textContent = getInitials(student.name);

      const rate = parseFloat(student.completion_rate || 0);
      const completed = parseInt(student.completed || 0, 10);
      const total = parseInt(student.total || 0, 10);

      statRateEl.textContent = rate.toFixed(1).replace('.', ',') + ' %';
      statCompletedEl.textContent = completed;
      statTotalEl.textContent = total;

      progressValueEl.textContent = rate.toFixed(1).replace('.', ',') + ' %';

      const width = Math.max(0, Math.min(100, rate));
      setTimeout(() => {
        progressBarEl.style.width = width + '%';
      }, 100);

      if (rate >= 100) {
        progressLabelEl.textContent = '✓ Parcours terminé';
      } else if (rate >= 60) {
        progressLabelEl.textContent = 'Progression avancée';
      } else {
        progressLabelEl.textContent = 'En cours';
      }

      // Afficher les modules
      modulesSectionEl.innerHTML = '';
      const modules = student.modules || {};

      Object.keys(modules).forEach((moduleName, index) => {
        const activities = modules[moduleName];
        const moduleCompletion = calculateModuleCompletion(activities);
        const activityCount = Object.keys(activities).length;

        const moduleCard = document.createElement('div');
        moduleCard.className = 'module-card';

        const moduleHeader = document.createElement('div');
        moduleHeader.className = 'module-header';
        moduleHeader.onclick = () => {
          const body = moduleCard.querySelector('.module-body');
          body.classList.toggle('active');
        };

        moduleHeader.innerHTML = `
          <div class="module-title">
            <div class="module-icon">${getModuleIcon(moduleName)}</div>
            <div class="module-info">
              <h4>${moduleName}</h4>
              <span class="module-meta">${activityCount} activité${activityCount > 1 ? 's' : ''}</span>
            </div>
          </div>
          <div class="module-stats">
            <span class="module-completion">${moduleCompletion}%</span>
            ${getModuleBadge(moduleCompletion)}
          </div>
        `;

        const moduleBody = document.createElement('div');
        moduleBody.className = 'module-body';

        const activityList = document.createElement('div');
        activityList.className = 'activity-list';

        Object.keys(activities).forEach(activityName => {
          const status = activities[activityName].trim();
          let label = status;
          let cssClass = 'pending';

          if (status.toLowerCase().includes('terminé') && status !== 'Pas terminé') {
            cssClass = 'done';
            label = 'Terminé';
          } else if (status === 'Pas terminé') {
            cssClass = 'pending';
            label = 'Pas terminé';
          }

          const activityItem = document.createElement('div');
          activityItem.className = 'activity-item';
          activityItem.innerHTML = `
            <span class="activity-name">${activityName}</span>
            <span class="status-badge ${cssClass}">${label}</span>
          `;
          activityList.appendChild(activityItem);
        });

        moduleBody.appendChild(activityList);
        moduleCard.appendChild(moduleHeader);
        moduleCard.appendChild(moduleBody);
        modulesSectionEl.appendChild(moduleCard);
      });
    }

    function showDashboard() {
      loginView.style.display = 'none';
      dashboardView.style.display = 'flex';
    }

    function showLogin() {
      dashboardView.style.display = 'none';
      loginView.style.display = 'flex';
      loginError.style.display = 'none';
      loginForm.reset();
      emailInput.focus();
      currentStudent = null;
      progressBarEl.style.width = '0%';
    }

    loginForm.addEventListener('submit', e => {
      e.preventDefault();
      const email = emailInput.value;
      const password = passwordInput.value;

      const student = findStudent(email, password);

      if (!student) {
        loginError.style.display = 'block';
        return;
      }

      loginError.style.display = 'none';
      updateDashboard(student);
      showDashboard();
    });

    logoutBtn.addEventListener('click', showLogin);
    printBtn.addEventListener('click', () => window.print());

  </script>
</body>
</html>
